{
  "name": "ercodingtheme",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build:style": "node-sass style.scss style.css --include-path node_modules --output-style expanded && postcss -r style.css",
    "build:style-editor": "node-sass style-editor.scss style-editor.css --include-path node_modules --output-style expanded && postcss -r style-editor.css",
    "build:scripts": "webpack",
    "build": "run-p \"build:* --mode=production\"",
    "build-dev": "run-p \"build:* --mode=development\"",
    "watch": "chokidar \"**/*.scss\" \"js/src/**/*.js\" -i \"node_modules/**/*\" -c \"yarn build-dev\" --initial",
    "lint:css": "stylelint \"**/*.scss\"",
    "fix:css": "stylelint \"**/*.scss\" --fix",
    "lint:js": "eslint .",
    "fix:js": "eslint . --fix",
    "lint": "run-p \"lint:*\"",
    "format": "prettier \"**/*.{php,js,scss,json}\" --write"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@prettier/plugin-php": "^0.24.0",
    "@wordpress/eslint-plugin": "^3.1.0",
    "babel-loader": "^8.0.6",
    "chokidar-cli": "^2.0.0",
    "node-sass": "^7.0.1",
    "normalize-scss": "^7.0.1",
    "npm-run-all": "4.1.5",
    "postcss-cli": "^6.0.1",
    "prettier": "^3.6.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "aos": "^3.0.0-beta.6",
    "autoprefixer": "^9.8.6",
    "bootstrap": "^4.5.2",
    "gsap": "^3.11.4",
    "morphext": "^2.4.7",
    "parallax-js": "^3.1.0",
    "slick-carousel": "^1.8.1",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
